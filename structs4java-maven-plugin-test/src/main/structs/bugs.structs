package org.structs4java.bugs;

struct SignessBug {
	int32_t signed32;
	uint32_t unsigned32;
	int16_t signed16;
	uint16_t unsigned16;
	int8_t signed8;
	uint8_t unsigned8;
}

struct Element {
	int16_t value;
}

struct CountOfBug {
	uint8_t num countof(elements);
	Element elements[];
}


struct Element2 {
	int16_t value;
	char    name[];
}

struct CountOfBug2 {
	uint8_t num countof(elements);
	Element2 elements[];
}
